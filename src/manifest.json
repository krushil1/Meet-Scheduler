{
  "name": "Meet Scheduler",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": true
  },
  "permissions": ["storage", "alarms", "tabs", "https://meet.google.com/"],
  "manifest_version": 2,
  
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
